#%RAML 1.0
title: QueryParameters API
version: v1
baseUri: https://api.codingmatters.org/query-parameters
mediaType: application/json

/root:
    displayName: RootResource
    get:
        queryParameters:
            stringParam:
                type: string
            stringArrayParam:
                type: array
                items: string
            intParam: integer
            intArrayParam:
                type: array
                items: integer
